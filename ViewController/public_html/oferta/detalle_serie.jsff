                       shortDesc="#{viewcontrollerBundle.OF_DESC_HISTOGRAMA}">
      <af:panelGroupLayout layout="scroll"
                           inlineStyle="width:100%; height:100%;">
        <af:spacer width="10" height="10"/>
        <af:outputText value="#{viewcontrollerBundle.OF_DESC_HISTOGRAMA}" 
                        inlineStyle="color:Red; font-family:Arial, Helvetica, sans-serif; font-size:14px; text-align:left;"
                       />
        <af:spacer width="10" height="10"/>
        <dvt:barGraph subType="BAR_VERT_CLUST"
                      tabularData="#{detalleEstacionBacking.tabularDataHistograma}"
                      dynamicResize="FIXED_SIZE" imageHeight="300"
                      imageWidth="1000" y1AxisTitle="#{detalleEstacionBacking.unidad}"
                      o1AxisTitle="Rangos">
		      
          <dvt:background>
            <dvt:specialEffects/>
          </dvt:background>
          <dvt:graphPlotArea/>
          <dvt:seriesSet >
            <dvt:series />
            
          </dvt:seriesSet>
          <dvt:o1Axis/>
          <dvt:y1Axis >
            <dvt:numberFormat numberFormatString="###.##"/>
          </dvt:y1Axis>
          <dvt:legendArea rendered="false"/>
          
        </dvt:barGraph>
      </af:panelGroupLayout>
    </af:showDetailItem>
    
    <af:showDetailItem text="Precipitación vs Evaporación"
                        rendered="#{detalleEstacionBacking.flagCompara}"
                        shortDesc="#{viewcontrollerBundle.OF_DESC_PvsE}">
         <af:panelGroupLayout layout="scroll"
                           inlineStyle="width:100%; height:100%;">
              <af:spacer width="10" height="10"/>
              <af:outputText value="#{viewcontrollerBundle.OF_DESC_PvsE}" 
                        inlineStyle="color:Red; font-family:Arial, Helvetica, sans-serif; font-size:14px; text-align:left;"
                       />
              <af:spacer width="10" height="10"/>
        <dvt:lineGraph subType="LINE_VERT_ABS"
                       tabularData="#{detalleEstacionBacking.tabularData}"
                       dynamicResize="FIXED_SIZE" imageWidth="1000"
                       imageHeight="300"
                       y1AxisTitle="Prec. vs Evap."
                       o1AxisTitle="Año">
          <dvt:background>
            <dvt:specialEffects/>
          </dvt:background>
          <dvt:graphPlotArea/>
          <dvt:seriesSet defaultMarkerShape="MS_CIRCLE"
                         defaultAssignedToY2="DY2_TRUE">
            <dvt:series markerShape="MS_DIAMOND" color="#ff0000" lineWidth="1"/>
            <dvt:series color="#3173ff"  lineWidth="1"/>
          </dvt:seriesSet>
          <dvt:o1Axis/>
          <dvt:y1Axis >
            <dvt:numberFormat numberFormatString="#####.##"/>
          </dvt:y1Axis>
          <dvt:legendArea position="LAP_BOTTOM"/>
        </dvt:lineGraph>
      </af:panelGroupLayout>
      
    </af:showDetailItem>
  </af:panelTabbed>
</jsp:root>
