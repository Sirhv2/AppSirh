            <dvt:specialEffects/>
          </dvt:background>
          <dvt:graphPlotArea/>
          <dvt:seriesSet>
            <dvt:series/>
          </dvt:seriesSet>
          <dvt:o1Axis/>
          <dvt:y1Axis >
            <dvt:numberFormat numberFormatString="#####.##"/>
          </dvt:y1Axis>
          <dvt:legendArea rendered="false"/>
        </dvt:barGraph>
