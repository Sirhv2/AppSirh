                      tabularData="#{detalleEstacionBacking.tabularDataFrecuencia}"
                      dynamicResize="FIXED_SIZE" imageHeight="300"
                      imageWidth="1000" y1AxisTitle="Nivel medio mensual(centimetros)"
                      o1AxisTitle="Años">
