                                                      actionListener="#{detalleTramoBean.cmi_metas_actionListener}"
                                                      action="#{detalleTramoBean.getAccionAdicionar}"
                                                      immediate="true"/> 
                                   
                                                      
                                </af:menu>
                              </f:facet>
                              <f:facet name="toolbar"/>
                              <f:facet name="statusbar"/>
                              <af:tree binding="#{detalleTramoBean.tree1}"
