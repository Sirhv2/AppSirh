                                                          unselectedLabel="#{viewcontrollerBundle.label_seleccione}"
                                                          value="#{detalleTramoBean.tramo.tipoFlujo}"
                                                          rendered="#{!detalleTramoBean.tramo.idFuente.esSubterranea}"
                                                          >
                                        <f:selectItems value="#{detalleTramoBean.tiposFlujo}"
                                                       binding="#{detalleTramoBean.selectItems4}"
                                                       id="selectItems4"/>
                                      </af:selectOneChoice>
                                      
                                      <af:selectOneChoice label="#{viewcontrollerBundle.TRM_AREA}"
                                                          binding="#{detalleTramoBean.selectOneChoice1}"
