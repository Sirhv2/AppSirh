                                       value="#{PuntosMonitoreoTreeHandler.treemodel}"
                                       contentDelivery="immediate" var="node"
                                       varStatus="nodeStatus"   >
                                <f:facet name="nodeStamp">
                                  <af:commandLink text="#{node.description} "
                                                  binding="#{detalleTramoBean.clink}"
                                                  id="clink"
                                                  action="#{node.getAction}"
                                                  actionListener="#{detalleTramoBean.clink_actionListener}">
                                               
                                                <f:attribute name="nombreParametro"
                                               value="#{node.nombreParametro}"/>
                                  <f:attribute name="valorParametro"
                                               value="#{node.data}"/>
                                  <f:attribute name="extraInfo"
                                               value="#{node.extraData}"/>
                                  <f:attribute name="extraInfo2"
                                               value="#{node.extraData2}"/>    
                                                  
                                 </af:commandLink>                 
                                </f:facet>
                              </af:tree>
                            </af:panelCollection>
                          </af:panelBox>
                        </af:panelGroupLayout>
                      </f:facet>
                      <f:facet name="start"/>
                      <f:facet name="end"/>
                      <f:facet name="top"/>
                    </af:panelStretchLayout>
                  </f:facet>
                </af:panelSplitter>
              </f:facet>

              <f:facet name="top">
                <af:panelGroupLayout layout="horizontal"
                                     xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
                                     binding="#{detalleTramoBean.pgl4}"
                                     id="pgl4">
                  <af:spacer width="10" height="10"
                             binding="#{detalleTramoBean.spacer7}"
                             id="spacer7"/>
                  <af:commandLink text="#{viewcontrollerBundle.FNT_TODAS}"
                                  binding="#{detalleTramoBean.commandLink1}"
                                  id="commandLink1" action="fuentes"/>
                  <af:spacer width="10" height="10"
                             binding="#{detalleTramoBean.spacer4}"
                             id="spacer4"/>
                  <af:commandLink text="#{viewcontrollerBundle.FNT_FUENTE} : #{detalleTramoBean.tramo.idFuente.nombre}"
                                  binding="#{detalleTramoBean.commandLink2}"
                                  id="commandLink2" action="detalleFuente"/>
                  <af:spacer width="50" height="10"
                             binding="#{detalleTramoBean.s3}" id="s3"/>
                  <af:commandLink text="#{viewcontrollerBundle.TRM_TODOS}"
                                  binding="#{detalleTramoBean.cl1}" id="cl1"
                                  action="listarTramos"
                                  immediate="true" partialSubmit="true"
                                  />
                  <af:spacer width="10" height="10"
                             binding="#{detalleTramoBean.s4}" id="s4"/>
                  <af:outputText value="#{viewcontrollerBundle.TRM_TRAMO} : #{detalleTramoBean.tramo.nombre}"
                                 binding="#{detalleTramoBean.ot1}" id="ot1"/>
                </af:panelGroupLayout>
                  
                  
                  
              </f:facet>
            </af:panelStretchLayout>
                
                
                
                
                
          </f:facet>
        </af:pageTemplate>
