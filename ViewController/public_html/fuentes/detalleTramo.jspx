                                          <af:convertNumber type="number"
                                                            integerOnly="false"
                                                            minIntegerDigits="1"
                                                            maxIntegerDigits="2"
                                                            minFractionDigits="0"
                                                            maxFractionDigits="3"
                                                             pattern="##.##"/>
                                       </af:inputText>
                                       </h:panelGrid>
