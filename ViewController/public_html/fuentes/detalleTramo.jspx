                                                    id="inputText4Aforo" 
                                                    columns="5"
                                                    value="#{detalleTramoBean.aforo.gradosLong}"
                                                    showRequired="true">
                                        <af:convertNumber type="number"
                                                          integerOnly="true"
                                                          minIntegerDigits="1"
                                                          maxIntegerDigits="2"/>
                                        <af:validateLongRange minimum="-82"
                                                              maximum="-66"/>
                                        </af:inputText>
                                        <af:inputText label="#{viewcontrollerBundle.MINUTOS}"
                                                      binding="#{detalleTramoBean.inputText5Aforo}"
                                                      id="inputText5Aforo"
                                                      columns="5" 
                                                       value="#{detalleTramoBean.aforo.minutosLong}" 
                                                       showRequired="true"
                                                      >
                                          <af:convertNumber type="number"
                                                            integerOnly="true"
                                                            minIntegerDigits="1"
                                                            maxIntegerDigits="2"/>
                                          <af:validateLongRange minimum="0" maximum="59"/>
                                          </af:inputText>
                                          <af:inputText label="#{viewcontrollerBundle.SEGUNDOS}"
                                                        binding="#{detalleTramoBean.inputText6Aforo}"
                                                        id="inputText6Aforo"
                                                        columns="5" 
                                                          value="#{detalleTramoBean.aforo.segundosLong}" 
                                                          showRequired="true"
                                                        >
                                            <af:convertNumber type="number"
                                                              integerOnly="false"
                                                              minIntegerDigits="1"
                                                              maxIntegerDigits="2"
                                                              minFractionDigits="0"
                                                              maxFractionDigits="3"
                                                               pattern="##.#####"/>
                                              <af:validateDoubleRange minimum="0.0"
                                                                      maximum="59.99999"/>
                                            </af:inputText>           
                                        </h:panelGrid>
                                        <h:panelGrid columns="2">
                                        <af:commandButton text="Relacionar"
                                                            id="commandButtonAforo"
                                                            icon="/imgs/add.png"
                                                            action="#{detalleTramoBean.adicionarAforo}"
                                                            windowHeight="16"/>
                                         <af:commandButton text="Cancelar"
                                                            id="commandButton3Aforo"
                                                            icon="/imgs/back.png"
                                                            immediate="true"
                                                            action="#{detalleTramoBean.finalizarAforo}"
                                                            windowHeight="16"/>
                                        </h:panelGrid>
                                      </h:panelGroup> 
                                      
                                      <af:table var="row"
                                                    binding="#{detalleTramoBean.tableAforo}"
                                                    id="tAforo"
                                                    value="#{detalleTramoBean.lsAforos}"
                                                    emptyText="#{viewcontrollerBundle.tabla_vacia}"
                                                     width="400px" 
                                                    >
                                                <af:column width="40px"  headerText="Caudal (l/s)">
                                                  <af:outputText value="#{row.caudal}"/>
                                                </af:column>
                                                <af:column width="65px" headerText="Fecha">
                                                  <af:outputText value="#{row.fecha}"/>
                                                </af:column>
                                                <af:column headerText="Coordenada" width="180px">
                                                  <af:outputText value="Lat #{row.gradosLat}° #{row.minutosLat}' #{row.segundosLat}''
                                                  - Long #{row.gradosLong}° #{row.minutosLong}' #{row.segundosLong}''"/>
                                                </af:column>
                                                
                                                 <af:column align="center" headerText="Borrar "  width="50px">
                                                 <h:commandLink 
                                                            id="borrarAforo"
                                                            immediate="true"
                                                            action="#{detalleTramoBean.borrarAforo}"
                                                            onclick="if(confirm('Esta seguro que desea eliminar este aforo?') == false){return false;}">
                                                         <h:graphicImage  value="/imgs/delete.png"/>
                                                         <f:param value="#{row.id}" name="caudalid"/>
                                                  </h:commandLink >
                                                 
                                                  
                                                  
                                                 </af:column>
                                      </af:table>
                                      </af:panelFormLayout>
                                  </f:facet>                                                        
                                </af:decorativeBox>
                                
                            
