                                                     id="outputText3"
                                                     styleClass="AFBrandingBarTitle"/>
                                    </af:panelGroupLayout>
                                    
                                    
                                  </f:facet>
                                </af:decorativeBox>
                              
                                <!--CDONCEL AFORO--> 
                                
                                <af:decorativeBox binding="#{detalleTramoBean.decorativeBoxAforo}"
