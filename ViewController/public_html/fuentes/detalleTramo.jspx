                                                       binding="#{detalleTramoBean.selectItems1}"
                                                       id="selectItems1"/>
                                      </af:selectOneChoice>
                                      <af:selectOneChoice label="#{viewcontrollerBundle.TRM_ZONA}"
                                                          binding="#{detalleTramoBean.selectOneChoice2}"
                                                          id="selectOneChoice2"
                                                          showRequired="true"
                                                          unselectedLabel="#{viewcontrollerBundle.label_seleccione}"
                                                          valueChangeListener="#{detalleTramoBean.selectOneChoice2_valueChangeListener}"
                                                          autoSubmit="true"
                                                          immediate="true"
                                                          value="#{detalleTramoBean.tramo.idZona}"
                                                          rendered="#{!detalleTramoBean.tramo.idFuente.esSubterranea}"
                                                          >
                                        <f:selectItems value="#{detalleTramoBean.listaZona}"
                                                       binding="#{detalleTramoBean.selectItems2}"
                                                       id="selectItems2"/>
                                      </af:selectOneChoice>
                                     
                                      <af:selectOneChoice label="#{viewcontrollerBundle.TRM_SUBZONA}"
