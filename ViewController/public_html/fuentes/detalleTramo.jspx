                                                              integerOnly="false"
                                                              minIntegerDigits="1"
                                                              maxIntegerDigits="5"
                                                              minFractionDigits="0"
                                                              maxFractionDigits="2"
                                                              pattern="#####.##"/>
                                      </af:inputText>
                                      <af:inputText label="#{viewcontrollerBundle.TRM_OFERTA}"
                                                    binding="#{detalleTramoBean.it_oferta}"
                                                    id="it_oferta" showRequired="true"
                                                    value="#{detalleTramoBean.tramo.ofertaHidricaTotal}"
                                                    rendered="#{!detalleTramoBean.tramo.idFuente.esSubterranea}"
                                                    >
                                            <af:convertNumber type="number"
                                                              integerOnly="false"
                                                              minIntegerDigits="1"
                                                              maxIntegerDigits="5"
                                                              minFractionDigits="0"
                                                              maxFractionDigits="2"
                                                              pattern="#####.##"/>  
                                      </af:inputText> 
                                       <!--CDONCEL campo observación-->
                                                   <af:inputText label="#{viewcontrollerBundle.TRM_OBS}"
