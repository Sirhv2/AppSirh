                                                          id="selectOneChoice3"
                                                          showRequired="true"
                                                          unselectedLabel="#{viewcontrollerBundle.label_seleccione}"
                                                          value="#{detalleTramoBean.tramo.idSubzona}"
                                                          rendered="#{!detalleTramoBean.tramo.idFuente.esSubterranea}"
                                                          >
                                        <f:selectItems value="#{detalleTramoBean.listaSubzona}"
                                                       binding="#{detalleTramoBean.selectItems3}"
                                                       id="selectItems3"/>
                                      </af:selectOneChoice>
                                       <!-- CDONCEL BOTON AFORO -->
                                            <af:commandButton text="Aforos"
                                                                  id="commandButton1Aforo"
